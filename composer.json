{
  "name": "sebastianlenz/craft-essentials",
  "description": "Essential services for Craft CMS",
  "type": "craft-plugin",
  "license": "MIT",
  "require": {
    "php": "^7.4.0",
    "ext-json": "*",
    "craftcms/cms": "^3.7.0",
    "sebastianlenz/craft-utils": "^2.1.0",
    "gettext/gettext": "^4.7"
  },
  "require-dev": {
    "sebastianlenz/contentfield": "dev-release",
    "solspace/craft3-calendar": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "lenz\\craft\\essentials\\": "src/"
    }
  },
  "extra": {
    "handle": "lenz-craft-essentials",
    "name": "Essentials",
    "developer": "Sebastian Lenz",
    "developerUrl": "https://github.com/sebastian-lenz/"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    },
    "platform": {
      "php": "7.4.14"
    }
  }
}
